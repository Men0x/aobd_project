name: aobdproject

conda_env: conda.yaml

entry_points:
  main:
    parameters:
      learning_rate: {type: float, default: 0.1}
      n_estimators: {type: int, default: 100}
      n_jobs: {type: int, default: 1}
    command: |
        python train.py \
          --learning-rate={learning_rate} \
          --n-estimators={n_estimators} \
          --n-jobs={n_jobs}
